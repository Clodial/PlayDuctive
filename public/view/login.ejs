<html>
<head>
	<!--Main front-end items-->
	<% include partials/head %>
	<title>PlayDuctive</title>
</head>
<body>
	<!--Page that shows what happens when the user's logged in-->
	<% include partials/nav %>
	<% if(proj){ 
		var chProj = eval(proj)
		for(var i = 0; i < chProj.length; i++){ 
			var piece = chProj[i] %>
			<div class="col-md-3">
				<p><%= piece.name %></p>
				<p><%= piece.stat %></p>
				<form action="/project" method="POST">
					<input type="hidden" name="projectId" value="<%= piece.id %>">
					<button type="submit">View Project</button> 
				</form>
			</div>
	<% } %>
	<% } else{ %>
		Oh no!
	<% } %>
	<form action="/makeProject" method="get">
		<button class="col-md-3" type="submit">Make Project</button>
	</form>
	<% include partials/bodyScript %>
</body>
</html>